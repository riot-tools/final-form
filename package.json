{
  "name": "riot-final-form",
  "version": "2.1.3",
  "description": "Riot HOC for using Final Form",
  "main": "index.umd.js",
  "module": "dist/index.esm.js",
  "js:next": "dist/index.esm.js",
  "types": "types.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "tdd": "npm run test -- --watch",
    "rollup": "rollup -c",
    "lint": "eslint lib",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/damusix/riot-final-form.git"
  },
  "keywords": [
    "riot",
    "final-form",
    "forms",
    "riot-4"
  ],
  "author": "Danilo Alonso (http://danilo.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damusix/riot-final-form/issues"
  },
  "homepage": "https://github.com/damusix/riot-final-form#readme",
  "peerDependencies": {
    "final-form": ">= 4.x.x",
    "riot": ">= 4.x.x"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-react": "^7.16.0",
    "final-form": "^4.20.1",
    "ghooks": "^2.0.4",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jsdom": "^15.2.1",
    "riot": "^4.6.6",
    "riot-jest-transformer": "^6.0.4",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "semantic-release": "^17.1.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "ci": false,
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
